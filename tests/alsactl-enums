#!/usr/bin/env python3

from sys import exit
import gi
gi.require_version('ALSACtl', '0.0')
from gi.repository import ALSACtl

types = {
}

for obj, types in types.items():
    for t in types:
        if not hasattr(obj, t):
            print('Enumerator {0} is not produced.'.format(t))
            exit(1)
